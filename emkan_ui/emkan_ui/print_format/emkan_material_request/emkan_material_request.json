 "html": "<table>\n    <thead>\n        <tr>\n            <td>\n                <div class=\"img\">\n                  <img src=\"/assets/emkan_ui/images/logo2.png\" alt=\"Emkan Engineering Logo\" style=\"width: 160px; height:130px;\">\n                </div>\n              \n            </td>\n        </tr>\n        <tr>\n            <td>\n                <div class=\"header\">\n                    <h2>MATERIAL REQUISITION AND ISSUE VOUCHER</h2>\n                </div>\n              \n            </td>\n        </tr>\n    </thead>\n    <tbody>\n        <tr>\n            <td>\n                <div class=\"container\">\n                    <div class=\"details\">\n                        <table>\n                            <tr>\n                                <td>PROJECT / JOB CODE:<span>NOT</span></td>\n                                <td style=\"width:30%;\">DATE:<span style=\"margin-left: 5px;\">{{doc.transaction_date or \"\"}}</span></td>\n                                <td>SERIAL NO. <span style=\"color:red ; font-size: 14px; margin-left: 5px;\">{{doc.name or \"\"}}</span></td>\n                            </tr>\n                        </table>\n                    </div>\n                    <div class=\"main\">\n                        <table class=\"main-table\">\n                            <thead>\n                                <tr>\n                                    <th  style=\"width:10%;\" >No.</th>\n                                    <th style=\"width:40%; text-align: center\"  >DESCRIPTION</th>\n                                    <th style=\"width:15%;\" >QUANTITY</th>\n                                    <th style=\"width:15%;\" >UNIT</th>\n                                    <th style=\"width:20%;\" >REMARKS</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                {% for row in doc.items%}\n                                <tr>\n                                    <td>{{row.idx}}</td>\n                                    <td>{{row.description}}</td>\n                                    <td>{{row.qty or \"\"}}</td>\n                                    <td>{{row.uom or \"\"}}</td>\n                                    <td></td>\n                                </tr>\n                                {% endfor %}\n                            </tbody>\n                        </table>\n        </div>\n                    <div class=\"notes\">\n                        <p>NOTES:</p>\n        </div>\n                    <div class=\"footer\">\n                        <table>\n                            <tr>\n                                <td></td>\n                                <td colspan=\"3\">REQUESTED BY</td>\n                                <td colspan=\"3\">CHECKED BY</td>\n                                <td colspan=\"3\">ISSUED BY</td>\n                                <td colspan=\"3\">RECEIVED BY</td>\n                            </tr>\n                               <tr>\n                                <td>NAME:</td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                            </tr>\n                            <tr>\n                                <td>POSITION:</td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                            </tr>\n                            <tr>\n                                <td>SIGNATURE:</td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                                <td colspan=\"3\"></td>\n                              