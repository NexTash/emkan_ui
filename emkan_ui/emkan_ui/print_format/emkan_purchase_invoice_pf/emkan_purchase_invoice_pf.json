 "creation": "2024-10-03 15:25:24.823999",
 "css": ".text-right{\n    text-align:right;\n}\n.text-left{\n    text-align:left;\n}\n\n.header{\n    display: flex;\n    gap: 20px!important;\n    padding: 15px 0px;\n    border-bottom: 1px dashed #7a7777;\n    border-bottom-width: 2px;\n}\n.highlight{\n    display:flex;\n    flex-direction:column;\n    justify-content:center;\n}\n.highlited{\n    background:#c0c0c0;\n    border:2px solid black;\n    padding:0px 50px;\n    border-radius:5px;\n    margin:0px 30px;\n}\n.bordered{\n    border:1px solid black;\n    border-radius:5px;\n}\n.center{\n    text-align:center;\n}\n.print-format{\n    padding:0px !important;\n}\n\n.account-heading{\n    margin:10px 50px;\n    padding:1px;\n    width:70%;\n    background-color:white;\n    border:1px solid #c0c0c0;\n    border-radius:50px;\n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Purchase Invoice",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<table style=\"width:100%;\">\n    <thead>\n        <tr>\n            <th style=\"padding:0px !important;\">\n                <header class=\"header\">\n                    <div class=\"user\" style=\"width:14%;\"></div>\n                    <div class=\"highlight\" style=\"width:62%;\">\n                        <div class=\"highlited\">\n                            <p class=\"center\">BSI International Building Systems WLL</p>\n                            <h5 class=\"center\" style=\"margin:0px !important;\"><b>AP Invoice Edit List</b></h5>\n                        </div>\n                        <div class=\"bordered\" style=\"width:100%;\">\n                            <p class=\"center\">Group ID: BOECKER1</p>\n                            <p class=\"center\">Book: BSI International Main Book Currency: QAR</p>\n                        </div>\n                    </div>\n                    <table style=\"width:20%;line-height:0 !important;height:60px;\">\n                      <tr>\n                        <td style=\"padding-left:0px !important;padding-right:0px !important;padding-bottom:0px !important;\">Page :</td>\n                        <td class=\"text-right\" style=\"padding-bottom:0px !important\">{{doc.idx}}</td>\n                      </tr>\n                      <tr>\n                        <td style=\"padding-left:0px !important;padding-right:0px !important;padding-bottom:0px !important\">Date:</td>\n                        <td class=\"text-right\" style=\"padding-bottom:0px !important\">{{ frappe.utils.formatdate(frappe.utils.nowdate(), \"dd-mm-yyyy\") }}</td>\n                      </tr>\n                      <tr>\n                        <td style=\"padding-left:0px !important;padding-right:0px !important;padding-bottom:0px !important\">Time:</td>\n                         {% set current_time = frappe.utils.now_datetime() %}\n                        <td class=\"text-right\" style=\"padding-bottom:0px !important\">{{ current_time.strftime('%H:%M:%S%p') }}</td>\n                      </tr>\n                    </table>\n                </header>\n            </th>\n        </tr>\n    </thead>\n    <tbody>\n        <tr>\n            <th>\n                <div class=\"info\" style=\"display:flex;gap:30px;\">\n                <div class=\"first\" style=\"display:flex;flex-direction:column !important;\">\n                <u class=\"heading\"><b>Supplier</b></u>    \n                <p>{{doc.supplier}}</p>\n                </div>\n                <div class=\"second\" style=\"display:flex;flex-direction:column !important;\">\n                <u class=\"heading\"><b>Name</b></u>\n                <p>{{doc.supplier_name}}</p>\n                </div>\n                </div>\n                <table style=\"width:100%\">\n              <tr>\n                <th class=\"text-left\"><u><b>Invoice</b></u></th>\n                <th class=\"text-right\"><u><b>DM</b></u></th>\n                <th class=\"center\"><u><b>Inv Date</b></u></th>\n                <th class=\"text-left\" style=\"width:93px;\"><u><b>Disc Date</b></u></th>\n                <th class=\"text-right\"><u><b>Discount</b></u></th>\n                <th class=\"text-left\"><u><b>Terms</b></u></th>\n                <th class=\"text-left\"><u><b>Fiscal</b></u></th>\n              </tr>\n              <tr>\n                <td style=\"font-size:11px;\">{{doc.bill_no}}</td>\n                <td style=\"font-size:11px;\"></td>\n                <td class=\"center\" style=\"font-size:11px;\">{{doc.bill_date}}</td>\n                <td class=\"text-left\" style=\"font-size:11px;\"></td>\n                <td class=\"text-right\" style=\"font-size:11px;\">0.00</td>\n                <td class=\"text-left\" style=\"font-size:11px;\">AS AGREED</td>\n                <td class=\"text-left\" style=\"font-size:11px;\">{% set date = doc.posting_date %}\n                                                                                {% set month = frappe.utils.formatdate(date, \"MM\") %}\n                                                                                {% set year = frappe.utils.formatdate(date, \"YYYY\") %}\n                                                                                {{ month }}/{{ year }}</span></p></td>\n            </tr>\n            </table>\n            <table style=\"width:100%\">\n            <tr>\n                <th class=\"text-right\"><u><b>PO</b></u></th>\n                <th class=\"text-left\" style=\"width:120px;\"><u><b>Packing Slip</b></u></th>\n                <th class=\"text-left\" style=\"width:117px;\"><u><b>Part Number</b></u></th>\n                <th class=\"text-left\" style=\"width:157px;\"><u><b>Description</b></u></th>\n                <th class=\"text-right\"><u><b>Quantity</b></u></th>\n                <th class=\"text-right\"><u><b>Unit Cost</b></u></th>\n                <th class=\"text-right\"><u><b>Amount</b></u></th>\n            </tr>\n            {% for row in doc.items %}\n            <tr>\n                <td class=\"text-right\" style=\"font-size:11px;\">{{ row.purchase_order or \"\" }}</td>\n                <td class=\"text-left\" style=\"font-size:11px;\">{{ row.purchase_receipt or \"\" }}</td>\n                <td class=\"text-left\" style=\"font-size:11px;\">{{row.item_code}}</td>\n                <td class=\"text-left\" style=\"font-size:11px;\"> {{row.description}}</td>\n                <td class=\"text-right\" style=\"font-size:11px;\">{{row.qty}}</td>\n                <td class=\"text-right\" style=\"font-size:11px;\">{{row.get_formatted(\"rate\")}}</td>\n                <td class=\"text-right\" style=\"font-size:11px;\">{{row.get_formatted(\"amount\")}}</td>\n            </tr>\n            {% endfor %}\n                <table style=\"width:100%\">\n                      <tr>\n                        <th class=\"text-right\"><b>Supplier Variance:</b></th>\n                        {% set po_qty = frappe.db.get_value(\"Purchase Order\", doc.items[0].purchase_order, \"total_qty\") %}\n                        {% if po_qty %}\n                        <td class=\"text-left\" style=\"font-size:11px;\">\n                            {{ po_qty - doc.total_qty }}\n                        </td>\n                        {% endif %}    \n                       <th class=\"text-right\"><b>Invoice Total:</b></th>\n                        <td class=\"text-left\" style=\"font-size:11px;\">{{doc.get_formatted(\"total\")}}</td>\n                        \n                    </tr>\n                </table>\n                <div class=\"book-total\" style=\"width:215px;\">\n                    <h5><b>BOOK TOTALS</b></h5>\n                    <p style=\"display:flex;\"><b>Number of Invoices:</b> <span style=\"margin-left:auto;\">1</span></p>\n                    <p style=\"display:flex;\"><b>Total Amount:</b> <span style=\"margin-left:auto;\">{{doc.get_formatted(\"total\")}}</span></p>\n                </div>\n                <div class=\"box\" style=\"border:1px solid black;padding:15px 20px;border-radius:5px;width:80%;\">\n\n                    <h5 class=\"account-heading center\"><b>GL ACCOUNT RECAP</b></h5>\n            <table style=\"width:100%;  border-collapse: separate;border-spacing: 0 2px;\">\n              <tr>\n                <th class=\"text-left\"><b>Account</b></th>\n                <th class=\"text-right\"><b>Debit</b></th>\n                <th class=\"text-right\"><b>Credit</b></th>\n              </tr>\n              <tr>\n                <td class=\"text-left\" style=\"font-size:11px;\">{{ doc.credit_to }}</td>\n                <td class=\"text-right\" style=\"font-size:11px;\">0.00</td>\n                <td class=\"text-right\" style=\"font-size:11px;\">{{ doc.get_formatted(\"total\") }}</td>\n              </tr>\n              \n              {% set account_totals = {} %}\n              {% for row in doc.items %}\n                {% set account = row.expense_account %}\n                {% if account_totals[account] is not defined %}\n                  {% set account_totals = account_totals.update({account: row.amount}) %}\n                {% else %}\n                  {% set account_totals = account_totals.update({account: account_totals[account] + row.amount}) %}\n                {% endif %}\n              {% endfor %}\n              \n              {% for account, total in account_totals.items() %}\n                <tr>\n                  <td class=\"text-left\" style=\"font-size:11px;\">{{ account }}</td>\n                  <td class=\"text-right\" style=\"font-size:11px;\">{{ frappe.format_value(total, {'fieldtype': 'Currency', 'options': doc.currency}) }}</td>\n                  <td class=\"text-right\" style=\"font-size:11px;\">0.00</td>\n                </tr>\n              {% endfor %}\n              \n              <tr>\n                <td></td>\n                <td class=\"text-right\" style=\"border-top:2px solid black;font-size:11px;\">\n                  {{ frappe.format_value(account_totals.values() | sum, {'fieldtype': 'Currency', 'options': doc.currency}) }}\n                </td>\n                <td class=\"text-right\" style=\"border-top:2px solid black;font-size:11px;\">{{ doc.get_formatted(\"total\") }}</td>\n                <td></td>\n              </tr>\n            </table>\n            </div>\n                        </th>\n                    </tr>\n            </tbody>\n        <tfoot></tfoot>\n    </table>",
